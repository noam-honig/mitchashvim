{
  "name": "mitchashvim",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server/server/",
    "build-local": "ng build && tsc -p tsconfig.server.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev-ng": "ng serve  --proxy-config proxy.conf.json",
    "dev-node": "ts-node-dev --project tsconfig.server.json src/server/",
    "release": "npm run build-local && copyfiles -V -a package.json dist/ && cd dist && git init && git add -A && git commit -m 'deploy' && git push -f https://git.heroku.com/mitchashvim.git master"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@remult/angular": "^0.4.10",
    "compression": "^1.7.3",
    "dotenv": "^8.1.0",
    "express": "^4.16.4",
    "express-jwt": "^6.0.0",
    "googlemaps": "^1.12.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "password-hash": "^1.2.2",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.12",
    "remult": "^0.4.10",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.8",
    "@angular/cli": "~12.2.8",
    "@angular/compiler-cli": "~12.2.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.16.1",
    "@types/express-jwt": "^6.0.1",
    "@types/googlemaps": "^3.43.3",
    "@types/jasmine": "~3.8.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^12.11.1",
    "@types/password-hash": "^1.2.20",
    "@types/pg": "^7.14.4",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "~4.3.5"
  },
  "browser": {
    "jsonwebtoken": false,
    "password-hash": false
  }
}
